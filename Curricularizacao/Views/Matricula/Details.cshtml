@model Curricularizacao.Models.Matricula

@{
    ViewData["Title"] = "Detalhes da Matrícula";
}

<div>
    <h4>Matrícula</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Aluno</dt>
        <dd class="col-sm-10">@Model.Aluno.Nome</dd>

        <dt class="col-sm-2">Atividade</dt>
        <dd class="col-sm-10">@Model.Atividade.Descricao</dd>

        <dt class="col-sm-2">Data da Matrícula</dt>
        <dd class="col-sm-10">@Model.DataMatricula.ToString("dd/MM/yyyy HH:mm:ss")</dd>
    </dl>
</div>

<div>
    <a asp-action="Index" class="btn btn-primary">Voltar</a>
</div>
